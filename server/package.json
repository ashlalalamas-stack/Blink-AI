{
  "name": "blink-server",
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "build:mac": "npm run build && pkg dist/index.js --targets node18-macos-x64 --output blink-macos"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1"
  },
  "devDependencies": {
    "@types/node": "^24.3.3",
    "pkg": "^5.8.1",
    "typescript": "^5.4.0"
  }
}
